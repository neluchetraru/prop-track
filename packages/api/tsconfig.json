{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "lib": ["ES2015", "DOM"],
    "target": "ES2015",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
} 